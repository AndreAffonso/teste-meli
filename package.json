{
  "name": "conduit-node",
  "version": "1.0.0",
  "description": "conduit on node",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "client-install": "cd client && npm install",
    "server": "node ./app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "setup": "npm run i  && npm run client-install && npm run dev",
    "test": "newman run ./tests/api-tests.postman.json -e ./tests/env-api-tests.postman.json",
    "stop": "lsof -ti :3000 | xargs kill"
  },
  "repository": {
    "type": "git"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "1.15.0",
    "concurrently": "^4.1.0",
    "cors": "2.7.1",
    "ejs": "2.4.1",
    "errorhandler": "1.4.3",
    "express": "4.13.4",
    "express-http-proxy": "^1.5.1",
    "express-jwt": "3.3.0",
    "express-session": "1.13.0",
    "jsonwebtoken": "7.1.9",
    "method-override": "2.3.5",
    "methods": "1.1.2",
    "morgan": "1.7.0",
    "request": "^2.81.0",
    "slug": "0.9.1",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "newman": "^3.8.2",
    "nodemon": "^1.11.0"
  }
}
